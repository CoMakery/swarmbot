{
  "name": "swarmbot",
  "version": "0.4.1",
  "private": true,
  "authors": [
    "Joel Dietz <joel.dietz@citizencode.io>",
    "Harlan T Wood <harlan@citizencode.io>",
    "Noah Thorp <noah@citizencode.io>",
    "Duke Dorje <duke@citizencode.io>"
  ],
  "description": "Bot for bounties",
  "scripts": {
    "colu": "colu",
    "deploy": "bin/deploy",
    "deploy-staging": "npm run fulltest && git branch --force staging head && git push --force --tags origin staging:staging",
    "deploy-production": "git fetch && git branch --force production origin/staging && git push --force --tags origin production:production",
    "dev": "bin/hubot --name s",
    "fulltest": "npm test && npm run lint",
    "lint": "coffeelint --quiet src test",
    "shipit": "if [[ $(git status --porcelain) ]]; then echo '*** Please stash or commit changes first ***\n'; else npm run push-if-green; fi ",
    "push-if-green": "npm run fulltest && git push --tags origin head",
    "start": "bin/hubot --adapter slack",
    "test": "mocha",
    "testv": "mocha --full-trace",
    "wrap": "npm shrinkwrap"
  },
  "devDependencies": {
    "chai": "^3.2.0",
    "chai-as-promised": "^5.1.0",
    "coffeelint": "github:harlantwood/coffeelint#arrow_hugging",
    "firebase-server": "^0.5.0",
    "mocha": "^2.2.5",
    "nock": "^3.3.2",
    "shelljs": "^0.5.3",
    "sinon": "^1.17.2"
  },
  "dependencies": {
    "airbrake": "^0.3.8",
    "bitcoinjs-lib": "^2.1.1",
    "bluebird": "^3.0.5",
    "coffee-script": "^1.10.0",
    "colu": "^0.9.7",
    "debug": "^2.2.0",
    "firebase": "^2.2.9",
    "hubot": "^2.16.0",
    "hubot-redis-brain": "0.0.3",
    "hubot-scripts": "^2.16.1",
    "hubot-slack": "^3.3.0",
    "instagram-node-lib": "0.1.1",
    "javascript-state-machine": "^2.3.5",
    "lightsaber": "^0.4.1",
    "lodash": "^3.10.0",
    "request-promise": "0.4.3",
    "validator": "^4.3.0"
  },
  "engines": {
    "node": "4.2.2",
    "npm": "3.3.12"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:citizencode/swarmbot.git"
  },
  "bugs": {
    "url": "https://github.com/citizencode/swarmbot/issues"
  }
}
